{
  "admin": {
    "disabled": true
  },
  "apps": {
    "http": {
      "servers": {
        "srv0": {
          "automatic_https": {
            "disable": true
          },
          "errors": {
            "routes": [
              {
                "handle": [
                  {
                    "body": "HTTP {http.error.status_code} Error",
                    "handler": "static_response"
                  }
                ]
              }
            ]
          },
          "listen": [
            ":80"
          ],
          "routes": [
            {
              "handle": [
                {
                  "browse": {},
                  "root": "/data",
                  "handler": "file_server",
                  "index_names": ["index.html", "index.txt", "index"]
                }
              ]
            }
          ]
        }
      }
    }
  }
}
